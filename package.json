{
  "name": "maze-game",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec ts-node server/src/server.ts",
    "dev:client": "cd client && python3 -m http.server 8000",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:server": "tsc -p server/tsconfig.json",
    "build:client": "echo 'Client build - copying files'",
    "build": "npm run build:server && npm run build:client",
    "start": "node server/dist/server.js",
    "deploy": "pm2 start ecosystem.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "oracledb": "^6.9.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.0",
